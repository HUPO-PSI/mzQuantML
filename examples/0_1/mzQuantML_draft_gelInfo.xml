<!-- PROTEIN LIST -->
<!------------------>
<!--  Note: for gel-based we don't have proteinList -->
<ProteinList>
	<protein AccesionRef=""><!--link to mzIdentML-->
		<groupReference ref="groupA">
			<mean_abundance> <!-- note: plus units -->
			<groupReferenceMember>
				<abundance/>
			</groupReferenceMember>	
		</groupReference>
		<groupReference ref="groupB">
		</groupReference>
		<proteinRatios>		
		</proteinRatios>
		
		<peptideReferenceList><!-- prohibited -->
			<peptideRef="P1">
				
		</peptideReferenceList>
	</protein>
</ProteinList>

<!------------------>
<!-- PEPTIDE LIST -->
<!------------------>
<PolyPeptideList>
	<!-- Note: One spot is equal to one polypeptide -->
	<Polypeptide id="P1" GelMatchedFeatureRef="X1">
		<identificationRef="" proteinAccession=""/><!--link to mzIdentML-->
		<cvparams..>
			ratio
			p-values
			group_correlation (Which way of ratio is: A/B or B/A...)
			...
		</cvparams..>
		
	</Polypeptide>
	<Polypeptide GelMatchedFeatureRef="X2">
		<identificationRef="" proteinAccession=""/><!--link to mzIdentML-->
	</Polypeptide>
</PolyPeptideList>



<!------------------>
<!-- FEATURE LIST -->
<!------------------>
<FeatureList>
	<!-- Note: Depending on potential file sizes may want a "count" for featureList-->
	<GelMatchedFeature id="X1">
		<masterXCoord="">
		<masterYCoord="">
		<group ref="A">
			<!-- CV VALUES: -->
			<!-- group has 
			average abundance
			standard deviation
			
			-->		
			<!-- Note: group member is equivalent to an image -->
			<group_member ref="1">
				<!-- CV VALUES: -->
				abundance experimentally calculated (+units) 
				intensity
				scores
				xCoord
				yCoord
				boundary points (in gel-based and optional in MS-based)
			</group_member>
			<group_member id="2">
			</group_member>
			<group_member id="3">
			</group_member>
			<group_member id="4">
			</group_member>
		</group>
		<group ref="B">	
		</group>	
	</MatchedFeatures>
<FeatureList>



