				<SpectrumIdentificationItemRef file_ref="mzID_Ref" spectrumIdentificationItem_ref="SII_35"/>
			</SpectraSet>
			<SpectraSet id="SPEC_SET_4">
				<SpectrumIdentificationItemRef file_ref="mzID_Ref" spectrumIdentificationItem_ref="SII_41"/>
