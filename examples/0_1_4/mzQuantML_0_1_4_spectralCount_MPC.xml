	</FeatureList>
	
	<QuantLayerList>
		<!-- PSM spectra counts (amino acid sequence with modifications) -->
		<QuantLayer ID="QL_PSM_spectra_counts">
			<DataType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</DataType>
			<RowIndex>frag1 frag2 frag3 frag4 frag5 frag6 frag7</RowIndex>
			<RowType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</RowType>
			<ColumnIndex>ASS1 ASS2</ColumnIndex>
			<ColumnType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</ColumnType>
			<DataMatrix>
				<Row index="1">1.0 2.0</Row>
				<Row index="2">1.0 2.0</Row>
				<Row index="3">1.0 2.0</Row>
				<Row index="4">1.0 2.0</Row>
				<Row index="5">1.0 2.0</Row>
				<Row index="6">1.0 2.0</Row>
				<Row index="7">1.0 2.0</Row>
			</DataMatrix>
		</QuantLayer>
		
		<!-- peptide quantification (only sequence, i.e. aggregation (sum) of modified and unmodified version of peptide) -->
		<QuantLayer ID="QL_peptide_quantification">
			<DataType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</DataType>
			<RowIndex>pep1 pep2 pep3 pep4 pep5</RowIndex>
			<RowType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</RowType>
			<ColumnIndex>ASS1 ASS2</ColumnIndex>
			<ColumnType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</ColumnType>
			<DataMatrix>
				<Row index="1">1.0 2.0</Row>
				<Row index="2">1.0 2.0</Row>
				<Row index="3">1.0 2.0</Row>
				<Row index="4">1.0 2.0</Row>
				<Row index="5">1.0 2.0</Row>
			</DataMatrix>
		</QuantLayer>
		
		<!-- protein quantification: aggregation(mean) of peptides -->
		<QuantLayer ID="QL_protein_quantification">
			<DataType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</DataType>
			<RowIndex>prot1 prot2 prot3</RowIndex>
			<RowType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</RowType>
			<ColumnIndex>ASS1 ASS2</ColumnIndex>
			<ColumnType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</ColumnType>
			<DataMatrix>
				<Row index="1">1.0 2.0</Row>
				<Row index="2">1.0 2.0</Row>
				<Row index="3">1.0 2.0</Row>
			</DataMatrix>
		</QuantLayer>
		
		<!-- experimental groups: referencing proteins -->
		<!--
		<QuantLayer ID="QL_experimental_groups">
			<DataType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</DataType>
			<RowIndex>prot_grp1 prot_grp2</RowIndex>
			<RowType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</RowType>
			<ColumnIndex>ASS1 ASS2</ColumnIndex>
			<ColumnType>
				<cvParam accession="" name="" cvRef=""></cvParam>
			</ColumnType>
			<DataMatrix>
				<Row index="1">1.0 2.0</Row>
				<Row index="2">1.0 2.0</Row>
			</DataMatrix>
		</QuantLayer>
		-->
		
	</QuantLayerList>
	
	<DataProcessingList>
		<dataProcessing id="DP1">
			<processingMethod>
				<cvParam accession="MS:1001486" name="data filtering" cvRef="PSI-MS" value=""/>
				<userParam name="myParam1"/>
			</processingMethod>
			<processingMethod>
				<cvParam accession="MS:1000543" name="data processing action" cvRef="PSI-MS"/>
			</processingMethod>
		</dataProcessing>
		<dataProcessing id="DP2">
			<processingMethod>
				<cvParam accession="MS:1001486" name="data filtering" cvRef="PSI-MS"/>
				<userParam name="myParam2"/>
			</processingMethod>
			<processingMethod>
				<cvParam accession="MS:1000543" name="data processing action" cvRef="PSI-MS"/>
			</processingMethod>
		</dataProcessing>
	</DataProcessingList>
</mzQuantML>
