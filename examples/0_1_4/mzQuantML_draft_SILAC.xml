<!--
Sketch of an example for using data matrices to represent quant layers
-->
<mzQuantML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://psidev.info/psi/pi/mzQuantML/0.1 ../../schema/mzQuantML_0_1_4.xsd"
 xmlns="http://psidev.info/psi/pi/mzQuantML/0.1">
	<cvList>
		<cv id="PSI-MS" fullName="Proteomics Standards Initiative Mass Spectrometry Vocabularies" URI="http://psidev.cvs.sourceforge.net/viewvc/*checkout*/psidev/psi/psi-ms/mzML/controlledVocabulary/psi-ms.obo" version="2.25.0"/>
		<cv id="UO" fullName="Unit Ontology" URI="http://obo.cvs.sourceforge.net/*checkout*/obo/obo/ontology/phenotype/unit.obo"/>
	</cvList>
	
	
	<AssayList id="AssayList1">
		<Assay id="B1" name="B1">
			<RawFiles>
				<RawFile location="../msmsdata/b1.raw"/>
			</RawFiles>
		</Assay>
		<Assay id="B_plus10">
			<RawFiles>
				<RawFile location="../msmsdata/b1.raw"/>
			</RawFiles>
			<Label>
				<Modification massDelta="10">
					<cvParam cvRef="" accession="" name=""/>
				</Modification>
			</Label>
		</Assay>		
		<Assay id="E1" name="E1">
			<RawFiles>
				<RawFile location="../msmsdata/e1.raw"/>
			</RawFiles>

		</Assay>

		<Assay id="E_plus10" name="E_plus10">
			<RawFiles>
				<RawFile location="../msmsdata/e1.raw"/>
			</RawFiles>
			<Label>
				<Modification massDelta="10">
					<cvParam cvRef="" accession="" name=""/>
				</Modification>
			</Label>
		</Assay>
		
	</AssayList>
	
	
	<StudyVariableList>
		<StudyVariable id="SV_group_B" name="Group B">
			<cvParam accession="MS:0000000 (study variable)" name="wildtype" cvRef="PSI-MS" value="1" unitAccession="TODO" unitCvRef="TODO" unitName="TODO"/>	
			<AssayRef assay_ref="B1"/>
			<AssayRef assay_ref="E1"/>
		</StudyVariable>
		<StudyVariable id="SV_group_E" name="Group E">
			<cvParam accession="MS:0000000 (study variable)" name="disease" cvRef="PSI-MS" value="1" unitAccession="TODO" unitCvRef="TODO" unitName="TODO"/>
			<AssayRef assay_ref="B_plus10"/>
			<AssayRef assay_ref="E_plus10"/>
		</StudyVariable>
		
	</StudyVariableList>
	
	
	<ProteinGroupList>
		<!-- Put in some similar structure for representing protein groups -->
		
	</ProteinGroupList>
	
	

	<ProteinList>
		
		
	</ProteinList>
	
	<PeptideList>
		<QuantLayerRef quantLayer_ref="QL_1"/>
		<Peptide id="Pep1">
			<PeptideSequence>IGDYAGIK</PeptideSequence>
			<Modification> <!-- As needed --></Modification>
		</Peptide>
		<!-- And so on -->
	</PeptideList>
	
	<FeatureList>
		
				
		<Feature id="feature548_B1" assay_ref="B1" charge="2" MZ="418.729214693827" RT="26.8685">	<!-- Note - intensity would be captured as a quant layer -->
			
			<!-- i.e. mass trace is space separated, with defined order -->
			<Masstrace>
				418.7139 418.7454 26.635 27.592 
				419.2045 419.2548 26.635 27.592 
				419.688 419.7517 26.635 27.528 
				420.2408 420.254 826.635 27.018
			</Masstrace>
		</Feature>
		
		<Feature id="feature549_B1" assay_ref="B_plus10" charge="2" MZ="428.729214693827" RT="27.8685">	<!-- Note - intensity would be captured as a quant layer -->
			
			<!-- i.e. mass trace is space separated, with defined order -->
			<Masstrace>
				418.7139 418.7454 26.635 27.592 
				419.2045 419.2548 26.635 27.592 
				419.688 419.7517 26.635 27.528 
				420.2408 420.254 826.635 27.018
			</Masstrace>			
		</Feature>
		
				
		<Feature id="feature548_E1" assay_ref="E1" charge="2" MZ="418.729214693827" RT="26.8685">	<!-- Note - intensity would be captured as a quant layer -->
			
			<!-- i.e. mass trace is space separated, with defined order -->
			<Masstrace>
				418.7139 418.7454 26.635 27.592 
				419.2045 419.2548 26.635 27.592 
				419.688 419.7517 26.635 27.528 
				420.2408 420.254 826.635 27.018
			</Masstrace>
			
		</Feature>
		
		<Feature id="feature549_E1" assay_ref="E_plus10" charge="2" MZ="428.729214693827" RT="26.8685">	<!-- Note - intensity would be captured as a quant layer -->
			
			<!-- i.e. mass trace is space separated, with defined order -->
			<Masstrace>
				418.7139 418.7454 26.635 27.592 
				419.2045 419.2548 26.635 27.592 
				419.688 419.7517 26.635 27.528 
				420.2408 420.254 826.635 27.018
			</Masstrace>
			
		</Feature>
		
		
		
	</FeatureList>
	
	
	
	<MatchedFeatureList>		
		<MatchedFeature id="MF_1">			
			<feature_refs>feature548_B1 feature549_B1 feature548_E1 feature549_E1 </feature_refs>
			<PeptideRef peptide_ref="Pep1"/>		
		</MatchedFeature>

		
	</MatchedFeatureList>
			
	<QuantLayerList>
		
		<!-- In this structure it is not possible to report ratios. It would simplify the schema if we do not support ratios but some people may expect them... for discussion -->
		<QuantLayer ID="QL_1">
			<DataType>
				<cvParam cvRef="FeatureAbundance" accession="TODO" name="TODO"/>
			</DataType>
			<RowIndex>MF_1</RowIndex>
			<RowType>
				<cvParam cvRef="MatchedFeature" accession="" name=""/>
			</RowType>
			<ColumnIndex>B1 B_plus10 E1 E_plus10</ColumnIndex>
			<ColumnType>
				<cvParam cvRef="Assay" accession="" name=""/>
			</ColumnType>
			<DataMatrix>
				<Row index="1">14302.6968	14861.93224	4684.063045	4805.380278</Row>
			</DataMatrix>
		</QuantLayer>			
		
	</QuantLayerList>
	
	
		
		
	
	
