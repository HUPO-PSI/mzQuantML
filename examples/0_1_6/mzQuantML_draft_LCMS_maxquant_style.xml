<!--
Sketch of an example for using data matrices to represent quant layers
-->
<mzQuantML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://psidev.info/psi/pi/mzQuantML/0.1 ../../schema/mzQuantML_0_1_6.xsd"
 xmlns="http://psidev.info/psi/pi/mzQuantML/0.1">
	<cvList>
		<cv id="PSI-MS" fullName="Proteomics Standards Initiative Mass Spectrometry Vocabularies"
			URI="http://psidev.cvs.sourceforge.net/viewvc/*checkout*/psidev/psi/psi-ms/mzML/controlledVocabulary/psi-ms.obo"
			version="2.25.0"/>
		<cv id="UO" fullName="Unit Ontology"
			URI="http://obo.cvs.sourceforge.net/*checkout*/obo/obo/ontology/phenotype/unit.obo"/>
	</cvList>

	<InputFiles>
		<RawFilesGroup id="raw1">
			<RawFile location="../msmsdata/b1.raw" id="r1"/>
		</RawFilesGroup>
		<RawFilesGroup id="raw2">
			<RawFile location="../msmsdata/e1.raw" id="r2"/>
		</RawFilesGroup>
		<IdentificationFiles>
			<IdentificationFile location="myidents.mzid" id="mzid1"/>
		</IdentificationFiles>

		<SearchDatabase location="c:\mydb.fasta" id="SD1">
			<DatabaseName><userParam name="mydb.fasta"/></DatabaseName>
		</SearchDatabase>
	</InputFiles>
	
	<AssayList id="AssayList1">
		<Assay id="B1" name="B1">
			<raw_files_group_ref>raw1</raw_files_group_ref>
		</Assay>
		<Assay id="E1" name="E1">
			<raw_files_group_ref>raw2</raw_files_group_ref>			
		</Assay>		
		
	</AssayList>


	<StudyVariableList>
		<StudyVariable id="SV_group_B" name="Group B">
			<cvParam accession="MS:0000000 (study variable)" name="concentration" cvRef="PSI-MS"
				value="1" unitAccession="TODO" unitCvRef="TODO" unitName="TODO"/>
			<AssayRef assay_ref="B1"/>
		</StudyVariable>


	</StudyVariableList>
	
	<AnalysisSummary>
		<cvParam cvRef="" accession="" name="analysis type:label free intensity"/>
	</AnalysisSummary>
	
	<ProteinGroupList id="PGList">
		<ProteinGroup id="HSP70_HSP80">
			<Protein_refs>HSP70 HSP80</Protein_refs>
			<!-- Probably want to allow CV params to specify leading protein etc -->
		</ProteinGroup>
		<AssayQuantLayer id="Protein_group_assay_quant_layer">
			<DataType>
				<cvParam cvRef="PSI-MS" accession="TODO" name="Protein abundance"/>
			</DataType>
			<ColumnIndex>B1 E1</ColumnIndex>
			<DataMatrix>
				<Row object_ref="HSP70_HSP80">5400 10800</Row>
			</DataMatrix>
		</AssayQuantLayer>
	</ProteinGroupList>
	

	
	<ProteinList id="ProtList">
		<Protein id="HSP70" accession="HSP70">
			<Peptide_refs>PEPTIDER MLEPTIDER</Peptide_refs>			
		</Protein>
		<Protein id="HSP80" accession="HSP80">
			<Peptide_refs>PEPTIDER MLEPTIDER</Peptide_refs>
		</Protein>
		
		<GlobalQuantLayer id="QL_ANOVA">
			<ColumnIndex>
				<Column index="0" processingStep_ref="feature_quantification">
					<DataType>
						<cvParam cvRef="" accession="" name="ANOVA"/>
					</DataType>
				</Column>
				<Column index="1" processingStep_ref="feature_quantification">
					<DataType>
						<cvParam cvRef="" accession="" name="Max fold change"/>
					</DataType>
				</Column>

			</ColumnIndex>		
			<DataMatrix>
				<Row object_ref="HSP70">0.034 4.56</Row>
			</DataMatrix>
		</GlobalQuantLayer>
		
		
		<!-- If QuantLayer is provided on ProteinGroups, probably would not need to include protein values as well - just shown here to enumerate examples -->
		<AssayQuantLayer id="Protein_assay_quant_layer">
			<DataType>
				<cvParam cvRef="PSI-MS" accession="TODO" name="Protein abundance"/>
			</DataType>
			<ColumnIndex>B1 E1</ColumnIndex>
			<DataMatrix>
				<Row object_ref="HSP70">5400 10800</Row>
				<Row object_ref="HSP80">5400 10800</Row>
			</DataMatrix>
		</AssayQuantLayer>
		
		
		
		
	</ProteinList>
	

	<PeptideList id="PepList">
		<Peptide id="PEPTIDER">
			<PeptideSequence>PEPTIDER</PeptideSequence>
			<!-- If feature refs are used, is allowed to have more than one feature from the same assay -->
			<!-- <Feature_refs>featureB_1 featureE_1</Feature_refs> -->
			<MatchedFeature_refs>MF1</MatchedFeature_refs>
		</Peptide>
		<Peptide id="MLEPTIDER">
			<PeptideSequence>MLEPTIDER</PeptideSequence>
			<MatchedFeature_refs>MF2</MatchedFeature_refs>
		</Peptide>
		
		
		<!-- In this example, there is a quant layer in PeptideList since MaxQuant sums intensities from different peptide charge states -->
		<AssayQuantLayer id="Peptide_Assay_quantlayer">
			<DataType>
				<cvParam cvRef="PSI-MS" accession="TODO" name="Abundance"/>
			</DataType>
			<ColumnIndex>B1 E1</ColumnIndex>
			<DataMatrix>
				<Row object_ref="PEPTIDER">254098 345345</Row>	<!-- i.e. in Assay B, PEPTIDER had abundance 254098 and in assay E it was 345345 -->
				<Row object_ref="MLEPTIDER">334098 545345</Row>
			</DataMatrix>			
		</AssayQuantLayer>
		
	</PeptideList>
	
	
	<FeatureList RawFileGroup_ref="raw1" id="FList1">
		<Feature id="featureB_1" charge="2" MZ="418.172" RT="26.8685"/>
		<Feature id="featureB_2" charge="2" MZ="421.572" RT="27.8685"/>
		<Feature id="featureB_3" charge="2" MZ="487.729" RT="26.9685"/>
		<Feature id="featureB_4" charge="2" MZ="518.827" RT="26.8685"/>
		
		
		<FeatureQuantLayer id="FQL1">
			<ColumnIndex>
				<Column index="0" processingStep_ref="feature_quantification">
					<DataType>
						<cvParam cvRef="PSI-MS" accession="TODO" name="intensity"/>
					</DataType>
				</Column>
				<Column index="1" processingStep_ref="feature_quantification">
					<DataType>
						<cvParam cvRef="PSI-MS" accession="TODO" name="raw abundance"/>
					</DataType>
				</Column>
				<Column index="2" processingStep_ref="feature_quantification">
					<DataType>
						<cvParam cvRef="PSI-MS" accession="TODO" name="resolution"/>						
					</DataType>
				</Column>
				<Column index="3" processingStep_ref="feature_quantification">
					<DataType>
						<cvParam cvRef="PSI-MS" accession="TODO" name="mass fractional part"/>						
					</DataType>
				</Column>
				
			</ColumnIndex>			
			<DataMatrix>
				<Row object_ref="featureB_1">23304.18097 16033.65284 55844.77344 0.994629</Row>
				<Row object_ref="featureB_2">93481.41456 98090.36288 NaN 0.146698</Row>
				<Row object_ref="featureB_3">193481.71622 298090.36288 NaN 0.546698</Row>
				<Row object_ref="featureB_4">181.43671 398090.36288 793556 0.146698</Row>
				
			</DataMatrix>
		</FeatureQuantLayer>
	</FeatureList>
	
	<FeatureList RawFileGroup_ref="raw2" id="FList2">
		<Feature id="featureE_1" charge="2" MZ="418.172" RT="26.8685"/>
		<Feature id="featureE_2" charge="2" MZ="421.572" RT="27.8685"/>
		<Feature id="featureE_3" charge="2" MZ="487.729" RT="26.9685"/>
		<Feature id="featureE_4" charge="2" MZ="518.827" RT="26.8685"/>
		
		<FeatureQuantLayer id="FQL2">
			<ColumnIndex>
				<Column index="0" processingStep_ref="feature_quantification">
					<DataType>
						<cvParam cvRef="PSI-MS" accession="TODO" name="intensity"/>
					</DataType>
				</Column>
				<Column index="1" processingStep_ref="feature_quantification">
					<DataType>
						<cvParam cvRef="PSI-MS" accession="TODO" name="raw abundance"/>
					</DataType>
				</Column>
				<Column index="2" processingStep_ref="feature_quantification">
					<DataType>
						<cvParam cvRef="PSI-MS" accession="TODO" name="resolution"/>						
					</DataType>
				</Column>
				<Column index="3" processingStep_ref="feature_quantification">
					<DataType>
						<cvParam cvRef="PSI-MS" accession="TODO" name="mass fractional part"/>						
					</DataType>
				</Column>
				
			</ColumnIndex>			
			<DataMatrix>
				<Row object_ref="featureE_1">23304.18097 16033.65284 55844.77344 0.994629</Row>
				<Row object_ref="featureE_2">93481.41456 98090.36288 NaN 0.146698</Row>
				<Row object_ref="featureE_3">193481.71622 298090.36288 NaN 0.546698</Row>
				<Row object_ref="featureE_4">181.43671 398090.36288 793556 0.146698</Row>
				
			</DataMatrix>
		</FeatureQuantLayer>
	</FeatureList>
	
	<MatchedFeatureList id="MFList1">
		<MatchedFeature id="MF1">
			<Feature_refs>featureB_1 featureE_1</Feature_refs> 
		</MatchedFeature>
		<MatchedFeature id="MF2">
			<Feature_refs>featureB_2 featureE_2</Feature_refs>	
		</MatchedFeature>
		
	</MatchedFeatureList>
		
	

	<SoftwareList>
		<Software id="MaxQuant" version="2.0.5">
			<cvParam cvRef="MS" accession="" name="MaxQuant" value=""/>
		</Software>
	</SoftwareList>

	<DataProcessingList>
		<DataProcessing id="feature_quantification" softwareRef="MaxQuant">
			<input_objects>raw1</input_objects>
			<output_objects>FQL1</output_objects>
			<ProcessingMethod order="1"><userParam name=""/></ProcessingMethod>
			<ProcessingMethod order="2">
				
				<cvParam cvRef="MS" accession="MS:1000035" name="peak picking"/>
			</ProcessingMethod>
		</DataProcessing>
		
	
	</DataProcessingList>

</mzQuantML>
