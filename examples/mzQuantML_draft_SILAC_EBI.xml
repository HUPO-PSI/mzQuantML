		<Assays>
			<Assay id="30min_rep1">
				<RawFiles>
					<!-- multiple raw files in case of pre-fractionation -->
					<RawFile id="raw1" name="raw1.raw" description="scx1"/>
					<!--filename? description optional-->
					<RawFile id="raw2" name="raw2.raw" description="scx2"/>
					<RawFile id="raw3" name="raw3.raw" description="scx3"/>
					<!-- mzML, raw -->
				</RawFiles>
				<QuantitationUnits>
					<!-- several quantitationUnits only if there are labels in a single RUN -->
					<QuantitationUnit id="light0min_1">
						<Label>
							<!-- optional if labelfree -->
							<cvParam name="SILAC_UNLABELED"/>
						</Label>
					</QuantitationUnit>
					<QuantitationUnit id="heavy30min">
						<Label>
							<!-- optional if labelfree -->
							<cvParam name="SILAC_KR6_HEAVY"/>
						</Label>
