		<!-- PROTOCOL -->
		<Protocol>
			<Measurement id=""/>
			<Type id="" description="">
				<!-- CV for reporter, precursor, also defining intra/inter-sample -->
				<subtype/>
				<!-- SILAC, iTRAQ, TMT -->
			</Type>
			<Ratios>  <!-- Need several ratios in the schema?! -->
			<Ratio id="ratio_30min_rep1" name="" description="" operator="">
				<Units/>
				<!-- for absolute quantification -->
				<Numerator>
					<QuantitationUnitRef ref="heavy30min_rep1"/>
					<Fixed/>
					<Accession/>
				</Numerator>
				<Denominator>
					<QuantitationUnitRef ref="light0min_1_rep1"/>
				</Denominator>
			</Ratio>
			<Ratio>
