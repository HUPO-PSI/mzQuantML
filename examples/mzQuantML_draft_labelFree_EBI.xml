<mzQuantML>
	<!-- ********* -->
	<!-- PROTEIN LIST -->
	<!-- ********* -->
	<ProteinList>
		<ProteinAmbiguityGroupQ id="PAG_1">
			<ProteinAmbiguityGroupedQuantitationResult ref="pag_ref" fileref="file_ref"/>
			<Abundance value=""/>
			<GroupedQuantitationResult ref="groupA">
				<MeanAbundance value=""/>
				<STD value=""/>
			</GroupedQuantitationResult>
			<GroupedQuantitationResult ref="groupB">
				<Abundance value=""/>
				<STD value=""/>
			</GroupedQuantitationResult>
			<QuantitationResult ref="GM_A_1">
				<Abundance>
					<cvParam name="RAW intensity" value=""/>
				</Abundance>
				<STD value=""/>
			</QuantitationResult>
			<QuantitationResult ref="GM_A_2">
				<Abundance value=""/>
				<STD value=""/>
			</QuantitationResult>
			<QuantitationResult ref="GM_A_3">
				<Abundance value=""/>
				<STD value=""/>
			</QuantitationResult>
			<QuantitationResult ref="GM_A_4">
				<Abundance value=""/>
				<STD value=""/>
			</QuantitationResult>
			<QuantitationResult ref="GM_A_5">
				<Abundance value=""/>
				<STD value=""/>
			</QuantitationResult>
			<QuantitationResult ref="GM_A_6">
				<Abundance value=""/>
				<STD value=""/>
			</QuantitationResult>
			<ProteinDetectionHypothesisQ id="PDH_1">
				<PeptideRefs>
					<PeptideRef ref="pep1"/>
					<PeptideRef ref="pep2"/>
				</PeptideRefs>
				<ProteinDetectionHypothesisRef ref="pdh_ref" fileref="file_ref"/>
				<!-- option 1 OR 2 -->
				<!-- option 1 -->
				<GroupedQuantitationResult ref="groupA">
					<MeanAbundance/>
					<STD value=""/>
				</GroupedQuantitationResult>
				<GroupedQuantitationResult ref="groupB">
					<Abundance value=""/>
					<STD value=""/>
				</GroupedQuantitationResult>
				<QuantitationResult ref="GM_A_1">
					<Abundance value=""/>
					<STD value=""/>
				</QuantitationResult>
				<QuantitationResult ref="GM_A_2">
					<Abundance value=""/>
					<STD value=""/>
				</QuantitationResult>
				<QuantitationResult ref="GM_A_3">
					<Abundance value=""/>
					<STD value=""/>
				</QuantitationResult>
				<QuantitationResult ref="GM_A_4">
					<Abundance value=""/>
					<STD value=""/>
				</QuantitationResult>
				<QuantitationResult ref="GM_A_5">
					<Abundance value=""/>
					<STD value=""/>
				</QuantitationResult>
				<QuantitationResult ref="GM_A_6">
					<Abundance value=""/>
					<STD value=""/>
				</QuantitationResult>
				<!-- option 2 -->
				<GroupComparisonRef id="A/B">
					<!-- ratio -->
					<STD value=""/>
				</GroupComparisonRef>
				<GroupMemberComparisonRef ref="GM_AB_1">
					<!-- ratio -->
					<STD value=""/>
				</GroupMemberComparisonRef>
				<GroupMemberComparisonRef ref="GM_AB_2">
					<Abundance value=""/>
					<STD value=""/>
				</GroupMemberComparisonRef>
				<GroupMemberComparisonRef ref="GM_AB_3">
					<Abundance value=""/>
					<STD value=""/>
				</GroupMemberComparisonRef>
				<GroupMemberComparisonRef ref="GM_AB_4">
					<Abundance value=""/>
					<STD value=""/>
				</GroupMemberComparisonRef>
				<GroupMemberComparisonRef ref="GM_AB_5">
					<Abundance value=""/>
					<STD value=""/>
				</GroupMemberComparisonRef>
				<GroupMemberComparisonRef ref="GM_AB_6">
					<Abundance value=""/>
					<STD value=""/>
				</GroupMemberComparisonRef>
				<!-- option 2 -->
			</ProteinDetectionHypothesisQ>
		</ProteinAmbiguityGroupQ>
	</ProteinList>
	
	<Groups>
		<Group id="groupingA_1" name="groupA">
			<GMRefs>
				<GMRef ref="GM_A_1"/>
				<GMRef ref="GM_A_2"/>
				<GMRef ref="GM_A_3"/>
			</GMRefs>
		</Group>
		<Group id="groupingA_2" name="groupA">
			<GMRefs>
				<GMRef ref="GM_A_4"/>
				<GMRef ref="GM_A_5"/>
				<GMRef ref="GM_A_6"/>
			</GMRefs>
		</Group>
	</Groups>
	
	<GroupMembers>
		<GroupMember id="gm1">
			<raw_files>
				<raw_file id="" name="" description=""/>
				<!-- mzML, raw -->
				<modification>0Da</modification>
				<identification/>
				<!-- mzIdentML -->
			</raw_files>
		</GroupMember>
		<GroupMember id="gm2">
			<raw_files>
				<raw_file id="" name="" description=""/>
				<!-- mzML, raw -->
				<modification>6Da</modification>
				<identification/>
				<!-- mzIdentML -->
			</raw_files>
		</GroupMember>
	</GroupMembers>
	
	<!-- **************** -->
	<!-- PEPTIDE LIST -->
	<!-- **************** -->
	<PeptideList>
		<Peptide id="pep1">
			<sequence/>
			<modifications/>
			<group>
			</group>
			<groupMember>
				<!-- it can be either a single run or in case it was a prefractionation it can be a collection of runs -->
				<ratio value=""/>
			</groupMember>
		</Peptide>
		<Peptide id="pep2">
			<sequence/>
			<modifications/>
			<group>
			</group>
			<groupMember>
				<ratio value=""/>
			</groupMember>
		</Peptide>
	</PeptideList>
	
	<protocol>
		<software_ref/>
		<type id="" description="">
			<!-- CV for reporter, precursor, also defining intra/inter-sample -->
			<subtype/>
			<!-- SILAC, iTRAQ, TMT -->
		</type>
		<ratio id="" name="" description="" operator="">
			<units/>
			<!-- for absolute quantification -->
			<numerator>
				<group_ref id="" operator="sum"/>
				<!-- or -->
				<groupmember_ref id="" operator="sum"/>
				<fixed/>
				<accession/>
			</numerator>
			<denominator/>
		</ratio>
		<min_number_pep_per_prot/>
		<normalisation>
			<cvParam/>
		</normalisation>
		<swParams>
			<!-- software specific parameters -->
			<cvParams/>
		</swParams>
	</protocol>
</mzQuantML>
