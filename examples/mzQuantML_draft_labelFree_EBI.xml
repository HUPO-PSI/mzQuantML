<MzQuantML>
	<!-- ********* -->
	<!-- PROTEIN LIST -->
	<!-- ********* -->
	<ProteinList>
		<ProteinAmbiguityGroupQ id="PAG_1">
			<ProteinAmbiguityGroupRef ref="pag_ref" fileref="file_ref"/>
			<!-- do we need more than one? if no, it should be in ProteinAmbiguityGroupQ -->
			<GroupedQuantitationResult StudyVariable_ref="groupA">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<GroupedQuantitationResult StudyVariable_ref="groupB">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<QuantitationResult assay_ref="QU_A_1">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</QuantitationResult>
			<ProteinDetectionHypothesisQ id="PDH_1">
				<PeptideRefs>
					<PeptideRef ref="pep1"/>
					<PeptideRef ref="pep2"/>
				</PeptideRefs>
				<ProteinDetectionHypothesisRef ref="pdh_ref" fileref="file_ref"/>
				<!-- do we need more than one? if no, it should be in ProteinDetectionHypothesisQ -->
				<GroupedQuantitationResult StudyVariable_ref="groupA">
					<Param measure_ref="" value="" unit=""/>
					<!-- one or more -->
					<!-- AND/OR -->
					<Param ratio_ref="" value=""/>
					<!-- one or more -->
				</GroupedQuantitationResult>
				<GroupedQuantitationResult StudyVariable_ref="groupB">
					<Param measure_ref="" value="" unit=""/>
					<!-- one or more -->
					<!-- AND/OR -->
					<Param ratio_ref="" value=""/>
					<!-- one or more -->
				</GroupedQuantitationResult>
				<QuantitationResult assay_ref="QU_A_1">
					<Param measure_ref="" value="" unit=""/>
					<!-- one or more -->
					<!-- AND/OR -->
					<Param ratio_ref="" value=""/>
					<!-- one or more -->
				</QuantitationResult>
			</ProteinDetectionHypothesisQ>
		</ProteinAmbiguityGroupQ>
	</ProteinList>
	<!-- **************** -->
	<!-- PEPTIDE LIST -->
	<!-- **************** -->
	<PeptideList>
		<Peptide id="pep1">
			<sequence/>
			<modifications/>
			<GroupedQuantitationResult StudyVariable_ref="groupA">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<GroupedQuantitationResult StudyVariable_ref="groupB">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<QuantitationResult assay_ref="QU_A_1">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</QuantitationResult>
		</Peptide>
		<Peptide id="pep2">
			<sequence/>
			<modifications/>
			<GroupedQuantitationResult StudyVariable_ref="groupA">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<GroupedQuantitationResult StudyVariable_ref="groupB">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<QuantitationResult assay_ref="QU_A_1">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</QuantitationResult>
		</Peptide>
	</PeptideList>
	<StudyVariables>
		<StudyVariable id="groupingA_1" name="groupA">
			<QuantitationUnits>
				<QuantitationUnit ref="QU_A_1"/>
				<QuantitationUnit ref="QU_A_2"/>
				<QuantitationUnit ref="QU_A_3"/>
			</QuantitationUnits>
		</StudyVariable>
		<StudyVariable id="groupingA_2" name="groupA">
			<QuantitationUnits>
				<QuantitationUnit ref="QU_A_4"/>
				<QuantitationUnit ref="QU_A_5"/>
				<QuantitationUnit ref="QU_A_6"/>
			</QuantitationUnits>
		</StudyVariable>
	</StudyVariables>
	<Assays>
		<Assay id="gm1">
			<RawFiles>
				<!-- multiple raw files in case of pre-fractionation -->
				<RawFile id="" name="" description=""/>
				<!-- mzML, raw -->
			</RawFiles>
			<QuantitationUnits>
				<!-- several quantitationUnits only if there are labels in a single RUN -->
				<QuantitationUnit id="">
					<Label>
						<!-- optional if labelfree -->
						<cvParam name="iTRAQ114"/>
						<Modification>0Da</Modification>
					</Label>
				</QuantitationUnit>
				<QuantitationUnit id="">
					<Label>
						<!-- optional if labelfree -->
						<cvParam name="iTRAQ116"/>
						<Modification>6Da</Modification>
					</Label>
				</QuantitationUnit>
			</QuantitationUnits>
			<Identification/> 	<!-- mzIdentML -->
		</Assay>
	</Assays>
	
	<!-- PROTOCOL -->
	<Protocol>
		
		<Type id="" description="">
			<!-- CV for reporter, precursor, also defining intra/inter-sample -->
			<subtype/>
			<!-- SILAC, iTRAQ, TMT -->
		</Type>
		<Ratio id="" name="" description="" operator="">
			<Units/>
			<!-- for absolute quantification -->
			<Numerator>
				<GrupedQuantitationResultRef id="" operator="sum"/>
				<!-- or -->
				<QuantitationResultRef id="" operator="sum"/>
				<Fixed/>
				<Accession/>
			</Numerator>
			<Denominator/>
		</Ratio>
		<Min_number_pep_per_prot/>
		<Normalization>
			<cvParam/>
		</Normalization>
		<SoftwareRef/>
		<SoftwareParams>
			<!-- software specific parameters -->
			<cvParams/>
		</SoftwareParams>
	</Protocol>
</MzQuantML>
