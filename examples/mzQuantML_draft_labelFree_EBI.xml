<mzQuantML>
	<!-- ********* -->
	<!-- PROTEIN LIST -->
	<!-- ********* -->
	<ProteinList>
		<ProteinAmbiguityGroupQ id="PAG_1">
			<ProteinAmbiguityGroupRef ref="pag_ref" fileref="file_ref"/>
			<!-- do we need more than one? if no, it should be in ProteinAmbiguityGroupQ -->
			<GroupedQuantitationResult study_variable_ref="groupA">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<GroupedQuantitationResult study_variable_ref="groupB">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<QuantitationResult assay_ref="QU_A_1">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</QuantitationResult>
			<ProteinDetectionHypothesisQ id="PDH_1">
				<PeptideRefs>
					<PeptideRef ref="pep1"/>
					<PeptideRef ref="pep2"/>
				</PeptideRefs>
				<ProteinDetectionHypothesisRef ref="pdh_ref" fileref="file_ref"/>
				<!-- do we need more than one? if no, it should be in ProteinDetectionHypothesisQ -->
				<GroupedQuantitationResult study_variable_ref="groupA">
					<Param measure_ref="" value="" unit=""/>
					<!-- one or more -->
					<!-- AND/OR -->
					<Param ratio_ref="" value=""/>
					<!-- one or more -->
				</GroupedQuantitationResult>
				<GroupedQuantitationResult study_variable_ref="groupB">
					<Param measure_ref="" value="" unit=""/>
					<!-- one or more -->
					<!-- AND/OR -->
					<Param ratio_ref="" value=""/>
					<!-- one or more -->
				</GroupedQuantitationResult>
				<QuantitationResult assay_ref="QU_A_1">
					<Param measure_ref="" value="" unit=""/>
					<!-- one or more -->
					<!-- AND/OR -->
					<Param ratio_ref="" value=""/>
					<!-- one or more -->
				</QuantitationResult>
			</ProteinDetectionHypothesisQ>
		</ProteinAmbiguityGroupQ>
	</ProteinList>
	<!-- **************** -->
	<!-- PEPTIDE LIST -->
	<!-- **************** -->
	<PeptideList>
		<Peptide id="pep1">
			<sequence/>
			<modifications/>
			<GroupedQuantitationResult study_variable_ref="groupA">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<GroupedQuantitationResult study_variable_ref="groupB">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<QuantitationResult assay_ref="QU_A_1">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</QuantitationResult>
		</Peptide>
		<Peptide id="pep2">
			<sequence/>
			<modifications/>
			<GroupedQuantitationResult study_variable_ref="groupA">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<GroupedQuantitationResult study_variable_ref="groupB">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</GroupedQuantitationResult>
			<QuantitationResult assay_ref="QU_A_1">
				<Param measure_ref="" value="" unit=""/>
				<!-- one or more -->
				<!-- AND/OR -->
				<Param ratio_ref="" value=""/>
				<!-- one or more -->
			</QuantitationResult>
		</Peptide>
	</PeptideList>
	<Study_Variables>
		<Study_Variable id="groupingA_1" name="groupA">
			<QuantitationUnits>
				<QuantitationUnit ref="QU_A_1"/>
				<QuantitationUnit ref="QU_A_2"/>
				<QuantitationUnit ref="QU_A_3"/>
			</QuantitationUnits>
		</Study_Variable>
		<Study_Variable id="groupingA_2" name="groupA">
			<QuantitationUnits>
				<QuantitationUnit ref="QU_A_4"/>
				<QuantitationUnit ref="QU_A_5"/>
				<QuantitationUnit ref="QU_A_6"/>
			</QuantitationUnits>
		</Study_Variable>
	</Study_Variables>
	<Assays>
		<Assay id="gm1">
			<raw_files>
				<!-- multiple raw files in case of prefractionation -->
				<raw_file id="" name="" description=""/>
				<!-- mzML, raw -->
			</raw_files>
			<QuantitationUnits>
				<!-- several quantitationUnits only if there are labels in a single RUN -->
				<QuantitationUnit id="">
					<label>
						<!-- optional if labelfree -->
						<cvParam name="iTRAQ114"/>
						<modification>0Da</modification>
					</label>
				</QuantitationUnit>
				<QuantitationUnit id="">
					<label>
						<!-- optional if labelfree -->
						<cvParam name="iTRAQ116"/>
						<modification>6Da</modification>
					</label>
				</QuantitationUnit>
			</QuantitationUnits>
			<identification/>
			<!-- mzIdentML -->
		</Assay>
	</Assays>
	
	<!-- PROTOCOL -->
	<protocol>
		<software_ref/>
		<type id="" description="">
			<!-- CV for reporter, precursor, also defining intra/inter-sample -->
			<subtype/>
			<!-- SILAC, iTRAQ, TMT -->
		</type>
		<ratio id="" name="" description="" operator="">
			<units/>
			<!-- for absolute quantification -->
			<numerator>
				<group_ref id="" operator="sum"/>
				<!-- or -->
				<groupmember_ref id="" operator="sum"/>
				<fixed/>
				<accession/>
			</numerator>
			<denominator/>
		</ratio>
		<min_number_pep_per_prot/>
		<normalisation>
			<cvParam/>
		</normalisation>
		<swParams>
			<!-- software specific parameters -->
			<cvParams/>
		</swParams>
	</protocol>
</mzQuantML>
