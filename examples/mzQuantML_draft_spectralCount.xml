<!------------------>
<!-- PROTEIN LIST -->
<!------------------>
<ProteinList>
	<protein AccesionRef=""><!--link to mzIdentML-->
		<groupReference ref="groupA">
			<mean_abundance> <!-- note: plus units -->
			<groupReferenceMember>
				<abundance/>
			</groupReferenceMember>	
		</groupReference>
		<groupReference ref="groupB">
		</groupReference>
		<proteinRatios>		
		</proteinRatios>
		
		<peptideReferenceList><!-- prohibited -->
			<peptideRef="P1">
				
		</peptideReferenceList>
	</protein>
</ProteinList>

<!------------------>
<!-- PEPTIDE LIST -->
<!------------------>
<PeptideList>
	<!-- Note: for spectral count 
	-->
	<peptide id="P1" matchedFeatureRef="X1"/>
		<sequence/>
		<modifications/>
		<identificationRef=""/><!--link to mzIdentML-->
		<cvparams..>
			ratio
			p-values
			group_correlation
			...
		</cvparams..>
		
	</peptide>
	<peptide matchedFeatureRef="X2"/>
		<sequence/>
		<modifications/>
		<identificationRef=""/><!--link to mzIdentML-->
	</peptide>
</PeptideList>



<!------------------>
<!-- FEATURE LIST -->
<!------------------>
<FeatureList>
	<!-- Note: for spectral count we don't have feature list -->
	<MatchedFeature id="X1">
		<masterRT=""><!-- optional -->
		<window="">
		<group ref="A"><!-- may have mean intensity-->
			<!-- CV VALUES: -->
			<!-- group has 
			mean intensity
			standard deviation
			rt of parent ion 
			m/z of parent ion
			charge state of parent ion (not mandatory)
			-->		
			<!-- Note: group member is equivalent to identification of the reporter ion -->
			<group_member ref="1">
				<!-- CV VALUES: -->
				abundance experimentally calculated (+units) 
				intensity
				scores
				.
				.
				m/z 
				boundary points (in gel-based)
			</group_member>
			<group_member id="2">
			</group_member>
			<group_member id="3">
			</group_member>
			<group_member id="4">
			</group_member>
		</group>
		<group ref="B">	
		</group>	
	</MatchedFeatures>
<FeatureList>



